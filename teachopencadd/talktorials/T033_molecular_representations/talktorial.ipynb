   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "200"
      ]
     },
    }
    ],
   "source": [
    "# add hydrogens\n",
    "aspirin_h = Chem.AddHs(aspirin)\n",
    "# generate conformers\n",
    "ps = rdDistGeom.ETKDGv3()\n",
    "ps.randomSeed = 0xd06f00d\n",
    "ps.numThreads = 10\n",
    "conf_ids = rdDistGeom.EmbedMultipleConfs(aspirin_h,200,ps)\n",
    "len(conf_ids)"
   ]
  },
